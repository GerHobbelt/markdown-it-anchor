{
  "name": "markdown-it-anchor",
  "version": "3.0.0-5",
  "description": "Header anchors for markdown-it.",
  "keywords": [
    "markdown-it-plugin"
  ],
  "homepage": "https://github.com/valeriangalliat/markdown-it-anchor",
  "license": "Unlicense",
  "author": {
    "name": "Val√©rian Galliat",
    "url": "http://val.codejam.info/"
  },
  "files": [
    "README.md",
    "UNLICENSE",
    "index.js",
    "demo.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/valeriangalliat/markdown-it-anchor.git"
  },
  "scripts": {
    "build": "babel index.es6.js -o index.js",
    "lint": "standard index.es6.js test.js",
    "prepublish": "npm run build",
    "test": "npm run lint && npm run build && babel-node test.js"
  },
  "dependencies": {
    "string": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "markdown-it": "github:GerHobbelt/markdown-it",
    "markdown-it-attrs": "github:GerHobbelt/markdown-it-attrs",
    "standard": "10.0.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-assign",
      "add-module-exports"
    ]
  },
  "tonicExampleFilename": "demo.js"
}
